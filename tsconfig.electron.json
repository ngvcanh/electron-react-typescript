{
  "compilerOptions": {
    "outDir": "dist",
    "allowJs": false,
    "isolatedModules": false,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "target": "es5",
    "strict": true,
    "sourceMap": false,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": false,
    "lib": [
      "DOM",
      "ES6"
    ]
  },
  "include": [
    "src/launch"
  ],
  "exclude": []
}