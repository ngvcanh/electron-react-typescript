{
  "name": "electron-ts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/launch/main.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "scripts": {
    "clean:dist:windows": "if exist dist rmdir /Q /S dist",
    "electron:webpack": "webpack --watch",
    "electron:launch": "electron .",
    "electron:start": "cross-env ENV_BUILDER=electron npm run electron:webpack",
    "electron:build": "cross-env ENV_BUILDER=electron npm run electron:webpack",
    "react:webpack": "webpack --watch",
    "react:webpack:dev": "webpack serve",
    "react:start": "cross-env ENV_BUILDER=react npm run react:webpack:dev",
    "react:build": "cross-env ENV_BUILDER=react npm run react:webpack",
    "start": "npm run clean:dist:windows && npm run react:start",
    "build": "npm run clean:dist:windows && npm run react:build"
  },
  "author": "Ken Nguyen <ngvcanh2014@gmail.com> (https://kensoni.vn/)",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/css-minimizer-webpack-plugin": "^3.0.1",
    "@types/electron": "^1.6.10",
    "@types/html-webpack-plugin": "^3.2.5",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/node": "^14.17.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/webpack-dev-server": "^3.11.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "dotenv": "^9.0.2",
    "electron": "^13.1.3",
    "electron-builder": "^22.11.7",
    "electron-reload": "^1.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^1.6.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-electron-reload": "^1.0.1",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
